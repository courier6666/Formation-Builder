        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseworkWPF"
        mc:Ignorable="d"
        Title="AddEquipmentToEquipmentListWindow" Height="450" Width="500">
    <Grid>
        <Grid x:Name="DisplayEquipmentPanel" Margin="0,0,0,85" Background="AliceBlue" Height="350">
            
        </Grid>
        <Button Name = "addEquipmentButton" Margin="10,369,305,29" Content="Add" Click="addEquipmentButton_Click">
            
        </Button>
        <Button x:Name = "cancelEquipmentButton" Margin="305,369,10,29" Content="Cancel" Click="cancelEquipmentButton_Click"/>
    </Grid>
</Window>
